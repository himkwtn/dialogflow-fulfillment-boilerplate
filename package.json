{
  "name": "dialogflow-fulfillment-boilerplate",
  "version": "1.0.0",
  "description": "Dialogflow Fulfillment Boilerplate using Express and Typescript",
  "main": "handler.js",
  "scripts": {
    "start": "export NODE_ENV=development && nodemon  --exec ts-node ./src/app.ts",
    "debug": "export NODE_ENV=development && nodemon  --inspect -r ts-node/register ./src/app.ts",
    "tunnel": "bash scripts/tunnel.sh",
    "test": "jest",
    "build": "tsc"
  },
  "dependencies": {
    "actions-on-google": "^2.5.0",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "cors": "^2.8.5",
    "dialogflow-fulfillment": "dialogflow/dialogflow-fulfillment-nodejs",
    "express": "^4.16.4",
    "ramda": "0.25",
    "yup": "^0.26.7"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/compression": "^0.0.36",
    "@types/cors": "^2.8.4",
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.12",
    "@types/node": "^8.0.57",
    "@types/ramda": "^0.25.46",
    "@types/yup": "^0.26.4",
    "dotenv": "^6.2.0",
    "jest": "^23.6.0",
    "nodemon": "^1.18.9",
    "ts-jest": "^23.10.5",
    "ts-node": "^7.0.1",
    "typescript": "^2.9.2"
  },
  "author": "https://github.com/himkwtn",
  "license": "MIT"
}
